                        <ins class="adsbygoogle"
                             style="display:inline-block;width:336px;height:280px"
                             data-ad-client="ca-pub-3861532892125732"
                             data-ad-slot="1561466494"></ins>
                        <script>
                            (adsbygoogle = window.adsbygoogle || []).push({});
                        </script>
                    </div>
                    {{ entity.text|raw }}
                </div>
                <div class="span4">
                    <h1>{{ entity.name }}</h1>
                    <a href="http://{{ entity.site }}" rel="nofollow" target="_blank">{{ entity.site }}</a>
                    <div class="h6">Контактные данные:</div>
                    <div style="padding-left: 15px;">
                        {{ entity.contacts|raw }}
                    </div>
                    <div class="project-info">
                        <div class="h6">Виды услуг:</div>
                        {{ entity.services|raw }}
                    </div>
